ini style.css
* {
    box-sizing: border-box;
}

body {
    margin: 0;
    font-family: Arial, sans-serif;
    background-color: #f2f2f2;
}

/* WRAPPER: TENGAH VERTIKAL &amp; HORIZONTAL */
.wrapper {
    min-height: 100vh;
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 20px;
}

/* CONTAINER */
.container {
    max-width: 800px;
    width: 100%;
    background: white;
    padding: 40px;
    border-radius: 12px;
    box-shadow: 0 10px 30px rgba(0,0,0,0.1);
    text-align: center;
}

/* TEKS */
h1 {
    color: #2c3e50;
    margin-bottom: 20px;
}

p {
    color: #555;
    font-size: 18px;
}

/* TOMBOL */
.button-group {
    margin-top: 25px;
}

button {
    background-color: #3498db;
    color: white;
    border: none;
    padding: 14px 22px;
    font-size: 16px;
    border-radius: 8px;
    cursor: pointer;
    margin: 10px;
    transition: all 0.3s ease;
}

button:hover {
    background-color: #2980b9;
    transform: translateY(-3px) scale(1.05);
    box-shadow: 0 8px 20px rgba(0,0,0,0.2);
}

button:active {
    transform: scale(0.95);
}

/* HP */
@media (max-width: 600px) {
    .container {
        padding: 25px;
    }

    h1 {
        font-size: 22px;
    }

    p {
        font-size: 16px;
    }

    button {
        width: 100%;
        margin: 8px 0;
    }
}

